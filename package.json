{
  "name": "siawsh-site-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/check-sa.js",
    "dev": "next dev",
    "build": "node -e \"console.log('Node:', process.version);\" && npm -v && next build",
    "postbuild": "next-sitemap",
    "build:no-pre": "node -e \"console.log('Node:', process.version);\" && npm -v && cross-env SKIP_SA_CHECK=1 next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "deps:check": "depcheck --skip-missing=true || true",
    "gen:b64": "base64 -i credentials/firebase-admin.json | tr -d '\n' || type -a base64"
  },
  "engines": {
    "node": ">=20.11.1 <21",
    "npm": ">=10"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-tooltip": "1.2.8",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "firebase": "12.2.1",
    "firebase-admin": "13.5.0",
    "lucide-react": "0.544.0",
    "next": "15.5.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.8",
    "gray-matter": "^4.0.3",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-gfm": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-rehype": "^11.1.1",
    "rehype-stringify": "^10.0.0"
    ,"rehype-sanitize": "^6.0.0"
    ,"rehype-slug": "^6.0.0"
    ,"rehype-autolink-headings": "^7.1.0",
    "image-size": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "depcheck": "1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "ts-morph": "^23.0.0",
    "globby": "^14.0.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
